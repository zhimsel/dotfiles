[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "bitbucket"]
	url = git@bitbucket.org:zhimsel/dotfiles.git
	fetch = +refs/heads/*:refs/remotes/bitbucket/*
[branch "master"]
	remote = bitbucket
	merge = refs/heads/master
[remote "github"]
	url = git@github.com:zhimsel/dotfiles.git
	fetch = +refs/heads/*:refs/remotes/github/*
