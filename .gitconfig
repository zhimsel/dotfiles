[user]
     email = zach@himsel.net
     name = Zach Himsel
[core]
     eol = lf
     autocrlf = input
     safecrlf = false
     editor = vim
     excludesfile = ~/.gitignore_global
[merge]
     tool = vimdiff
[push]
     default = matching
[alias]
     unstage = reset HEAD --
     last = log -1 HEAD
     s = status
     f = fetch
     c = commit
     l = log
     a = add
     ck = checkout
     co = checkout
     br = branch
     undo-last-commit = reset --soft HEAD~1
