[user]
	email = z.himsel@gmail.com
	name = Zach Himsel
[core]
	autocrlf = input
	safecrlf = true
	editor = vim
[merge]
	tool = vimdiff
[push]
	default = upstream
[alias]
	unstage = reset HEAD --
	last = log -1 HEAD
	s = status
	f = fetch
	c = commit
	l = log
	a = add
	ck = checkout
	br = branch
	undo-last-commit = reset --soft HEAD~1
